{
    "name": "azure-nodejs-sample",
    "version": "v1",
    "services": [
      {
        "name": "nodejs-sample",
        "type": "kubernetesService",
        "properties": {
          "runtime": "node"
        },
        "source": "sample-mean-master"
      }
    ],
    "addons": [
      {
        "serverName":"sample-acr",
        "type": "containerRegistry",
        "authType": "servicePrinciple"
      },
      {
        "serverName": "sample-cosmos",
        "databaseName": "mydb",
        "type": "cosmos-mongodb",
        "authType": "secret"
      }
    ],
    "environments": {
      "dev": {
        "location": "eastus",
        "resourceGroup": "azure-sample-node"
      }
    }
  }